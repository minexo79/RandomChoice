    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Img\icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Img\add.png" />
    <None Remove="Img\minus.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Img\icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Img\add.png" />
    <Resource Include="Img\minus.png" />
  </ItemGroup>

</Project>
